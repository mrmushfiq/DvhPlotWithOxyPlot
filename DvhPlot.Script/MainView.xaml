<UserControl
    x:Class="DvhPlot.Script.MainView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    >
    <Grid>
        <ItemsControl
            ItemsSource="{Binding Structures}"
            >
            <ItemsControl.ItemTemplate>
                <DataTemplate>
                    <CheckBox
                        Content="{Binding Id}"
                        />
                </DataTemplate>
            </ItemsControl.ItemTemplate>
        </ItemsControl>
    </Grid>
</UserControl>
